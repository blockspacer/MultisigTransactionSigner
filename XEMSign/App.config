             type="XEMSign.MyConfigSection, XEMSign" />
    <section name="accountsBlackList"
            type="XEMSign.MyConfigSection, XEMSign" />
  </configSections>
  
 <!--Edit only below this line-->
  
  <accountsPrivate>
